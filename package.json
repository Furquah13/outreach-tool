{
  "name": "outreach-tool",
  "version": "0.1.0",
  "description": "Outreach Tool API v0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "render-build": "npm install && npx prisma generate && npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "node prisma/seed.js",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "outreach",
    "api",
    "express",
    "nodejs",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
